{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "CreateServiceRoles",
      "Effect": "Allow",
      "Action": [
        "iam:CreateRole",
        "iam:TagRole",
        "iam:GetRole",
        "iam:AttachRolePolicy"
      ],
      "Resource": "arn:aws:iam::690261714640:role/service-role/*"
    },
    {
      "Sid": "PassServiceRolesToAllowedServices",
      "Effect": "Allow",
      "Action": "iam:PassRole",
      "Resource": "arn:aws:iam::690261714640:role/service-role/*",
      "Condition": {
        "ForAnyValue:StringEquals": {
          "iam:PassedToService": [
            "lambda.amazonaws.com",
            "build.apprunner.amazonaws.com"
          ]
        }
      }
    }
  ]
}
